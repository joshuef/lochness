{
  "name": "lochness",
  "version": "1.1.1",
  "description": "Live form matrix for react component UI libraries",
  "main": "",
  "bin": {
    "loch": "bin/loch.js",
    "visualTest": "bin/visualTests.js",
    "loch:build": "bin/build.js"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint-fix": "npm run lint -- --fix",
    "bin:build": "babel lib --out-dir bin --source-maps",
    "bin:watch": "babel lib --watch --out-dir bin --source-maps",
    "start": "yarn run bin:build && node bin/loch.js -e dev",
    "test": "karma start",
    "test:v:ref": "node bin/visualTests.js -e dev -t reference",
    "test:v": "node bin/visualTests.js -e dev ",
    "test:visuals": "build && node bin/visualTests.js -e dev",
    "build": "node bin/build.js",
    "test:visuals:ref": "yarn dist && node bin/visualTests.js -e dev -t reference",
    "tdd": "karma start --autoWatch=true --singleRun=false",
    "dist": "yarn run copy & webpack --env=dist",
    "clean": "rimraf dist/*",
    "bump": "yarn pre-bump && yarn version && yarn post-bump",
    "pre-bump": "git checkout Release && git pull && git merge master --no-ff --no-edit && yarn bin:build && git add . ; git commit -m 'compiled dist & bin files';",
    "post-bump": "git checkout master && git cherry-pick --strategy=recursive -X theirs Release && git push"
  },
  "license": "MIT",
  "keywords": [],
  "author": "Sociomantic",
  "repository": {
